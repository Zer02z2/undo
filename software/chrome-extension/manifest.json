{
  "manifest_version": 2,
  "name": "Remote control",
  "version": "1.0.0",
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["injectTag.js", "content.js"]
    }
  ],
  "permissions": ["scripting"],
  "host_permissions": ["<all_urls>"],
  "web_accessible_resources": ["content.js"]
}
